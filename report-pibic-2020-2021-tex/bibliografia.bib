@article{chenZhao13,
    author = {Chen, Long and Da, Zhi and Zhao, Xinlei},
    title = "{What Drives Stock Price Movements?}",
    journal = {The Review of Financial Studies},
    volume = {26},
    number = {4},
    pages = {841-876},
    year = {2013},
    month = {02},
    abstract = "{A central issue in finance is whether stock prices move because of revisions in expected cash flows or discount rates, and by how much of each. Using direct cash flow forecasts, we show that stock returns have a significant cash flow news component whose importance increases with the investment horizon. For horizons over two years, cash flow news is more important. These conclusions hold at both the firm and aggregate levels, and diversification plays a secondary role in affecting the relative importance of cash flow and discount rate news. Our findings highlight the importance of cash flows in asset pricing.}",
    issn = {0893-9454},
    doi = {10.1093/rfs/hht005},
    url = {https://doi.org/10.1093/rfs/hht005},
    eprint = {https://academic.oup.com/rfs/article-pdf/26/4/841/24432096/hht005.pdf},
}

